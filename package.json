{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "vue-inertia-composable",
  "version": "0.4.1",
  "description": "Inertia for Vue2 Composition API.",
  "keywords": [
    "inertia",
    "laravel",
    "vue2",
    "composition-api"
  ],
  "license": "MIT",
  "author": {
    "name": "Logue",
    "email": "logue@hotmail.co.jp",
    "url": "https://logue.dev/"
  },
  "homepage": "https://github.com/logue/vue-inertia-composable",
  "repository": {
    "type": "git",
    "url": "git@github.com:logue/vue-inertia-composable.git"
  },
  "bugs": {
    "url": "https://github.com/logue/vue-inertia-composable/issues"
  },
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "/dist"
  ],
  "engines": {
    "node": ">=18.13.0",
    "yarn": ">=1.22.19"
  },
  "packageManager": "yarn@3.3.1",
  "scripts": {
    "clean": "rimraf node_modules/.vite",
    "type-check": "tsc --noEmit --composite false",
    "lint": "eslint . --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint && prettier . --write",
    "build": "run-p type-check build-only build-declaration",
    "build:analyze": "vite build --mode analyze",
    "build:clean": "rimraf dist",
    "build-only": "vite build",
    "build-declaration": "tsc -p tsconfig.app.json --composite false --declaration --declarationMap --declarationDir dist",
    "prepare": "husky install",
    "version": "auto-changelog -p && git add CHANGELOG.md"
  },
  "dependencies": {
    "vue-demi": "latest"
  },
  "peerDependencies": {
    "@inertiajs/inertia": "*",
    "@inertiajs/vue2": "^1.0.0",
    "@vue/composition-api": "^1.7.1",
    "vue": "^2.6.14 || ^3.0.0",
    "ziggy-js": "*"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@inertiajs/inertia": "^0.11.1",
    "@inertiajs/vue2": "^1.0.0",
    "@types/node": "^18.11.19",
    "@types/ziggy-js": "^1.3.2",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.33.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^39.8.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "rimraf": "^4.1.2",
    "rollup-plugin-visualizer": "^5.9.0",
    "typescript": "^4.9.5",
    "vite": "4.1.1",
    "vite-plugin-banner": "^0.7.0",
    "vite-plugin-checker": "^0.5.5",
    "vue": "^2.7.14",
    "vue-eslint-parser": "^9.1.0",
    "vue-tsc": "1.0.24",
    "ziggy-js": "^1.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,md}": "eslint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint",
    "*": "prettier -w -u"
  },
  "resolutions": {
    "json5": "^2.2.3",
    "http-cache-semantics": "^4.1.1"
  }
}
